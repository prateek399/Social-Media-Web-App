<%- include("partials/header"); -%>
<h1>Home</h1>
<p><%=homeMessage%></p>
<% allPosts.forEach((post)=>{ %>
<h1><%=post.title%></h1>
<p>
   <%=`${post.message.slice(0,100)}...`%><a
      href="posts/<%=post._id%>"
      style="color: rgb(1, 60, 255)"
      >Read more</a
   >
</p>
<% }) %> <%- include("partials/footer"); -%>
